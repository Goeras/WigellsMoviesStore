<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<StackPane xmlns="http://javafx.com/javafx"
           xmlns:fx="http://javafx.com/fxml"
           fx:controller="org.dreamteam.wigellsmoviesstore.Controllers.StartController"
           prefHeight="400.0" prefWidth="600.0"
            stylesheets="@styles/styles.css">
  <VBox styleClass="background">
    <VBox styleClass="body" minHeight="400">
      <HBox styleClass="header">
        <Label fx:id="topLabel" text="Wigells Film och Snarr"/>
      </HBox>
      <VBox styleClass="boxes" minHeight="250">
        <ChoiceBox fx:id="storeBox"/>
        <Button text="Meny" onAction="#onMenuButtonClick"/>
      </VBox>
    </VBox>
  </VBox>
</StackPane>
